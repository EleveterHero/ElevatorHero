using UnityEngine;
using System.Collections;
using UnityEngine.UI;
using System.Collections.Generic;

public class BackSprite : MonoBehaviour {

    static Dictionary<string,Sprite> g_static_sprites = null;

    Image m_image = null;

    bool move = false;

    Image image
    {
        get
        {
            if(m_image == null)
            {
                m_image = transform.FindChild("BattleBack").gameObject.GetComponent<Image>();
            }
            return m_image;
        }
    }


    Sprite primary_sprite = null;

    void Awake()
    {
        if(g_static_sprites == null)
        {
           g_static_sprites = new Dictionary<string, Sprite>();


            foreach (Sprite sp in Resources.LoadAll<Sprite>("Image/BattleBack"))
            {
                g_static_sprites.Add(sp.name, sp);
            }

        }
    }



	// Use this for initialization
	void Start () {
        GetComponent<Image>().rectTransform.sizeDelta = new Vector2(0.0f, 600.0f);
	}
	
	// Update is called once per frame
	void Update () {
	
	}
}
